<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>All Categories</title>
</head>

<body>
  <h1>All Categories</h1>

  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
      </tr>
    </thead>

    <tbody>
      <% categories.forEach(c=> { %>
        <tr>
          <td>
            <%= c.id %>
          </td>
          <td>
            <%= c.name %>
          </td>
        </tr>
        <% }) %>
    </tbody>
  </table>
</body>

</html>